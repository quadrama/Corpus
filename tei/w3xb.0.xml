                              <stage rend="zenoPC" xml:id="tg89.2.4">
                                 <hi rend="italic" xml:id="tg89.2.4.1">Gedämpftes Licht.</hi>
                              </stage>
                              <stage rend="zenoPC" xml:id="tg89.2.5">
                                 <hi rend="italic" xml:id="tg89.2.5.1">Sie starrt in Gedanken.</hi>
