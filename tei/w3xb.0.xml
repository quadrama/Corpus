                        <div type="text" xml:id="tg91.2">
                           <div type="h4">
                              <head type="h4" xml:id="tg91.2.1">V</head>
                              
                              
                              <stage rend="zenoPC" xml:id="tg91.2.4">
                                 <hi rend="italic" xml:id="tg91.2.4.1">Tag. Verhängte Fenster.</hi>
                              </stage>
                              <stage rend="zenoPC" xml:id="tg91.2.5">
                                 <hi rend="italic" xml:id="tg91.2.5.1">Toter aufgebahrt auf dem Diwan.</hi>
                              </stage>
                              <stage rend="zenoPC" xml:id="tg91.2.6">
                                 <hi rend="italic" xml:id="tg91.2.6.1">Sie kniet das Haupt auf dem Toten.</hi>
                              </stage>
                              <stage rend="zenoPC" xml:id="tg91.2.7">
                                 <hi rend="italic" xml:id="tg91.2.7.1">Freundin tappt hinein an den Türpfeiler gelehnt.</hi>
                              </stage>
                              <stage rend="zenoPC" xml:id="tg91.2.8">
                                 <hi rend="italic" xml:id="tg91.2.8.1">
                                    <pb n="222" xml:id="tg91.2.8.1.1"/>
Sie hebt den Kopf, wendet langsam, mechanisch, faßt die Freundin ins Auge, fährt übers Gesicht.</hi>
                              </stage>
                              <lb xml:id="tg91.2.9"/>
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
