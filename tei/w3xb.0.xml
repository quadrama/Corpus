                        <div type="text" xml:id="tg87.2">
                           <div type="h4">
                              <head type="h4" xml:id="tg87.2.1">I</head>
                              
                              
                              <stage rend="zenoPC" xml:id="tg87.2.4">
                                 <hi rend="italic" xml:id="tg87.2.4.1">Die Parkbäume spielen Schatten durch Fenster und Tür.</hi>
                              </stage>
                              <stage rend="zenoPC" xml:id="tg87.2.5">
                                 <hi rend="italic" xml:id="tg87.2.5.1">Sie am offenen Fenster, starrt hinaus, wendet jäh, stemmt die Faust.</hi>
                              </stage>
                              <stage rend="zenoPC" xml:id="tg87.2.6">
                                 <hi rend="italic" xml:id="tg87.2.6.1">Frauenlachen aus dem Park.</hi>
