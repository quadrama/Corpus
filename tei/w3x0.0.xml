                        <div type="text" xml:id="tg84.2">
                           <div type="h4">
                              <head type="h4" xml:id="tg84.2.1">[Stücktext]</head>

                              
                              
                              <stage rend="zenoPC" xml:id="tg84.2.4">
                                 <hi rend="italic" xml:id="tg84.2.4.1">Zimmer im Gasthof.</hi>
                              </stage>
                              <stage rend="zenoPC" xml:id="tg84.2.5">
                                 <hi rend="italic" xml:id="tg84.2.5.1">Zwei Betten nebeneinander; an der Wand gegenüber Flügeltür; an der Hinterwand zwischen hohen Fenstern ein Spiegel. Reisetaschen und Kleidungstücke auf Nachttischen und Stühlen verstreut.</hi>
                              </stage>
                              <stage rend="zenoPC" xml:id="tg84.2.6">
                                 <hi rend="italic" xml:id="tg84.2.6.1">Sie im vorderen Bett, richtet sich auf und starrt in das Dunkel.</hi>
                              </stage>
                              <lb xml:id="tg84.2.7"/>
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
                              
